{
    "name": "rezak/otp-auth",
    "description": "A Laravel package for OTP endpoint implementation",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "otp",
        "authentication",
        "laravel-package",
        "sms-otp"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rezak9876/otp-auth/issues",
        "source": "https://github.com/rezak9876/otp-auth"
    },
    "authors": [
        {
            "name": "RezaK",
            "email": "rezak9876.official@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RezaK\\OtpAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RezaK\\OtpAuth\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "require": {
        "php": "*",
        "ichtrojan/laravel-otp": "^2.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "orchestra/testbench": "^9.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "RezaK\\OtpAuth\\Providers\\OtpAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    }
}
